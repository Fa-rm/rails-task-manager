<h1>Edit Task</h1>
<%= form_with model: @task, local: true do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <br>
  <%= form.label :details %>
  <%= form.text_field :details %>
  <br>
  <%= form.label :completed %>
  <%= form.check_box :completed %>
  <br>
  <%= form.submit %>
<% end %>
<p><%= link_to "homepage", tasks_path %>  </p>
